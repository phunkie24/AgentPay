# AgentPay Environment Configuration Template
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to git!

# Blockchain Configuration
ALCHEMY_API_KEY=your_alchemy_api_key_here
ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_KEY
MNEE_CONTRACT_ADDRESS=0x8ccedbAe4916b79da7F3F612EfB2EB93A2bFD6cF

# Database - Use strong passwords in production!
SQL_PASSWORD=CHANGE_THIS_TO_STRONG_PASSWORD

# Redis - Use strong passwords in production!
REDIS_PASSWORD=CHANGE_THIS_TO_STRONG_PASSWORD

# RabbitMQ - Use strong credentials in production!
RABBITMQ_USER=CHANGE_THIS_USERNAME
RABBITMQ_PASS=CHANGE_THIS_TO_STRONG_PASSWORD

# AI/LLM Configuration
AI_PROVIDER=Ollama
OLLAMA_MODEL=llama3.2:latest
OLLAMA_BASE_URL=http://localhost:11434

# Optional: OpenAI (if not using Ollama)
OPENAI_API_KEY=your_openai_key_here

# Application
ASPNETCORE_ENVIRONMENT=Development
APP_NAME=AgentPay

# Security Notes:
# 1. Generate strong random passwords for production
# 2. Never use default or example passwords
# 3. Rotate credentials regularly
# 4. Use Azure Key Vault or similar for production secrets
# 5. Keep this file (.env) in .gitignore
